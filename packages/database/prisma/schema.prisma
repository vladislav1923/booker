datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id              String   @id @default(cuid())
  first_name      String
  last_name       String
  email           String   @unique
  password_digest String
  created_at      DateTime @default(now())
  updated_at      DateTime @default(now())
}
